// 文档教程: https://uniapp.dcloud.net.cn/uniCloud/schema
{
	"bsonType": "object",
	"required": ["schedule_id", "ranking", "pts"],
	"description": "每场分站设置的竞猜积分表",
	"permission": {
		"read": false,
		"create": false,
		"update": false,
		"delete": false
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"schedule_id": {
			"bsonType": "string",
			"description": "分站id"
		},
		"ranking": {
			"bsonType": "int",
			"description": "名次"
		},
		"pts": {
			"bsonType": "int",
			"description": "积分"
		}
	}
}
