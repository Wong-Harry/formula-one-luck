// 文档教程: https://uniapp.dcloud.net.cn/uniCloud/schema
{
  "bsonType": "object",
  "description": "用户每站竞猜表",
  "required": [],
  "permission": {
    "read": false,
    "create": false,
    "update": false,
    "delete": false
  },
  "properties": {
    "_id": {
      "description": "ID，系统自动生成"
    },
    "user_id": {
      "bsonType": "string",
      "description": "用户id"
    },
    "guess_id": {
      "bsonType": "string",
      "description": "竞猜条目id"
    },
    "guess_result": {
      "description": "竞猜结果"
    },
    "guess_state": {
      "enum": ["已开奖", "未开奖"],
      "defaultValue": "未开奖",
      "description": "竞猜状态"
    }

  }
}
